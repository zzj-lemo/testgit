/**index.wxss**/

/* 轮播 */
.swiper{
  width: 750rpx;
  height: 340rpx;
}
.swiper image {
  width: 100%;
  height: auto;
}

.cates{
  display: flex;
}

.cates navigator{
  flex: 1;
  padding: 20rpx;
}

.cates navigator image{
  width: 100%;
}

.floor .floor_group .floor_title image{
  width: 100%;
}

.floor .floor_group .floor_list{
  overflow: hidden;
  padding: 0 10rpx;
}

.floor .floor_group .floor_list navigator{
  float: left;
  width: 33.33%;
}

.floor .floor_group .floor_list navigator:nth-last-child(-n+4){
  height: 27vw;
  border-left: 10rpx solid #ffffff;
  box-sizing: border-box;
}

.floor .floor_group .floor_list navigator:nth-child(2),.floor .floor_group .floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid #ffffff;
}

.floor .floor_group .floor_list navigator image{
  width: 100%;
  height: 100%;
}