page{
    background-color: #dedede;
    padding: 20rpx;
    box-sizing: border-box;
}
.search_row{
    height: 60rpx;
    display: flex;
}
input{
    background-color: #fff;
    flex: 1;
    height: 100%;
    margin-right: 20rpx;
    padding-left: 20rpx;
}
button{
    background-color: #fff;
    width: 110rpx;
    height: 100%;
    font-size: 28rpx;
    padding: 0;
    line-height: 60rpx;
}
.search_content{
    margin-top: 30rpx;
}
.search_item{
    background-color: #fff;
    font-size: 26rpx;
    padding: 15rpx 10rpx;
    border-bottom: 1rpx solid #ccc;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}